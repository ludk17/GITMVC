@{
    ViewBag.Title = "Inicio";
}

<h2>Productos</h2>
<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Code</th>
            <th>Name</th>
        </tr>
    </thead>   
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Code</td>
                <td>@item.Name</td>
            </tr>
        }
    </tbody>
</table>

